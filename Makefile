
test:
	cargo test -- --nocapture

build:
	cargo build --all --target wasm32-unknown-unknown --release && cp ./target/wasm32-unknown-unknown/release/near_registry.wasm ./out/

dev_build_and_deploy:
	cargo build --all --target wasm32-unknown-unknown --release \
 && cp ./target/wasm32-unknown-unknown/release/near_registry.wasm .D/out/ \
  && near deploy -f --accountId dev-1651572692245-79836229473317 --wasmFile out/near_registry.wasm \
   && near call dev-1651572692245-79836229473317 init '{"owner_id": "near_registry.testnet"}' --accountId near_registry.testnet

build_and_deploy: test fmt lint
	cargo build --all --target wasm32-unknown-unknown --release \
 && cp ./target/wasm32-unknown-unknown/release/near_registry.wasm ./out/ \
  && near deploy -f --accountId near_registry.testnet --wasmFile out/near_registry.wasm \
   && near call near_registry.testnet init '{"owner_id": "near_registry.testnet", "dao": "dev-1657612598159-16577393546261"}' --accountId near_registry.testnet

test_init:
	near call dev-1653928999170-97878349269541 init '{"owner_id": "voting_registry_token.testnet", "ft_token": "voting_registry_token.testnet"}' --account_id voting_registry_token.testnet

test_call:
	near call dev-1653928999170-97878349269541 new_registry '{"owner_id": "near_registry.testnet", "data": {"name": "John Doe","age": 43}, "name": "test1"}' --accountId voting_registry_token.testnet

test_voting_add:
	near call dev-1653928999170-97878349269541 --account_id voting_registry_token.testnet add_proposal '{"proposal": {"owner": "voting_registry_token.testnet", "description": "Some description", "kind": "Vote", "registry_data": {"name": "John Doe","age": 44, "name": "test"}, "unique_identifier": "test.testnet"}}' --depositYocto 1000000000000000000000000

test_vote:
	near call dev-1653928999170-97878349269541 act_proposal '{"id": 0, "action": "VoteApprove", "amount": "1"}' --account_id ft-manager.testnet

test_delegate:
	near call dev-1653928999170-97878349269541 --account_id ft-manager.testnet delegate '{"account_id": "ft-manager.testnet", "amount": "1000000000"}'

